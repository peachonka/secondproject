{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Secret": "${JWT_SECRET}"
  },
  "ReverseProxy": {
    "Clusters": {
      "user-service": {
        "Destinations": {
          "server1": { "Address": "http://user-service:80" }
        }
      },
      "order-service": {
        "Destinations": {
          "server1": { "Address": "http://order-service:80" }
        }
      }
    }
  }
}